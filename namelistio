   field_5  = 'Ice temperature                    ', 'iicetemp', 'C       ',    1   ,  1.0     , -273.15
   field_6  = 'Oceanic flux at the ice base       ', 'ioceflxb', 'w/m2    ',    1   ,  1.0     ,    0.0
   field_7  = 'Salt flux at the ice base          ', 'iocesafl', 'kg/m2/s ',    1   ,  1.0     ,    0.0
   field_8  = 'Ice velocity u                     ', 'iicevelu', 'm/s     ',    1   ,  1.0     ,    0.0
   field_9  = 'Ice velocity v                     ', 'iicevelv', 'm/s     ',    1   ,  1.0     ,    0.0
   field_10 = 'Wind stress u over ice             ', 'iocestru', 'Pa      ',    1   ,  1.0     ,    0.0
   field_11 = 'Wind stress v over ice             ', 'iocestrv', 'Pa      ',    1   ,  1.0     ,    0.0
